<?xml version="1.0" encoding="utf-8" ?>


<odoo>
    <data noupdate="1">
        <!-- Sequence for generating unique Student IDs -->
        <record id="seq_school_student" model="ir.sequence">
            <field name="name">School Student Sequence</field>
            <field name="code">school.student.sequence</field>
            <field name="prefix">STU</field>
            <field name="padding">6</field>
        </record>
    </data>

    <data>
        <record id="seq_Kindergarten_classes" model="ir.sequence">
            <field name="name">Kindergarten Classes Sequence </field>
            <field name="code">kindergarten.classes.sequence</field>
            <field name="prefix">KG</field>
            <field name="padding">2</field>
        </record>

        <record id="seq_Elementary_classes" model="ir.sequence">
            <field name="name">Elementary Classes Sequence </field>
            <field name="code">elementary.classes.sequence</field>
            <field name="prefix">EL</field>
            <field name="padding">2</field>
        </record>

        <record id="seq_Basic_classes" model="ir.sequence">
            <field name="name">Basic Classes Sequence </field>
            <field name="code">basic.classes.sequence</field>
            <field name="prefix">BA</field>
            <field name="padding">2</field>
        </record>

        <record id="seq_Secondary_classes" model="ir.sequence">
            <field name="name">Secondary Classes Sequence </field>
            <field name="code">secondary.classes.sequence</field>
            <field name="prefix">SE</field>
            <field name="padding">2</field>
        </record>

    </data>
</odoo>